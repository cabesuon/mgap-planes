{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "planes-core-lib": [
        "dist/planes-core-lib/planes-core-lib",
        "dist/planes-core-lib"
      ],
      "planes-secano-lib": [
        "dist/planes-secano-lib/planes-secano-lib",
        "dist/planes-secano-lib"
      ],
      "seguros-secano-lib": [
        "dist/seguros-secano-lib/seguros-secano-lib",
        "dist/seguros-secano-lib"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
